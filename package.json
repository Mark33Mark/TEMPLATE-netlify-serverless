{
  "name": "template-react-vite",
  "version": "0.0.0",
  "author": "Mark Watson <mark@watsonised.com> (https://get.watsonised.com)",
  "description": "netlify serverless in react with vite server",
  "license": "MIT",
  "private": true,
  "type": "module",
  "keywords": [
    "vite",
    "css",
    "sass",
    "react"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && vite build",
    "prebuild": "rm -rf dist/ && rm -rf build/",
    "preview": "vite preview",
    "vitest-preview": "vitest-preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:debug": "npm-run-all -p test vitest-preview",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@jest/expect-utils": "^29.7.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "jest-matcher-utils": "^29.7.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.79.4",
    "vite": "^5.4.8",
    "vite-plugin-react": "^4.0.1",
    "vitest": "^2.1.2",
    "vitest-preview": "^0.0.1"
  }
}
